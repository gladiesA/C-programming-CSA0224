#include <stdio.h>
int main()
{
    float a, b, c;
    float root1, root2;
    printf("Enter values of a, b and c: ");
    scanf("%f %f %f", &a, &b, &c);
    root1 = (-b + sqrt(b*b - 4*a*c)) / (2*a);
    root2 = (-b - sqrt(b*b - 4*a*c)) / (2*a);
    printf("Root1 = %.2f\n", root1);
    printf("Root2 = %.2f\n", root2);
    return 0;
}
